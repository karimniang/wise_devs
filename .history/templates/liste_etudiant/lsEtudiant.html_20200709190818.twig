{% extends 'base.html.twig' %}


{% block body %}
	<h1>Hello Voici la liste des etudiants ! âœ…</h1>
	<table id="deuxiemeTable_id">
		<thead>
			<tr>
				<th>Nom</th>
				<th>Prenom</th>
				<th>Email</th>
				<th>Telephone</th>
			</tr>
		</thead>
		<tbody>
			{% for etudiant in etudiants %}
				<tr>
					<td>{{etudiant.nom}}</td>
					<td>{{etudiant.prenom}}</td>
					<td>{{etudiant.email}}</td>
					<td>{{etudiant.telephone}}</td>
